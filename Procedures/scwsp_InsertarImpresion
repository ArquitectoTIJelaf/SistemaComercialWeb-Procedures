Alter PROCEDURE scwsp_InsertarImpresion (
@id_venta			int,
@nom_tip_venta		varchar(25),
@emp_ruc			varchar(11),
@emp_raz_social		varchar(60),
@emp_direccion		varchar(100),
@emp_dir_agencia	varchar(150),
@emp_telefono1		varchar(15),
@emp_telefono2		varchar(15),
@cod_documento		varchar(2),
@boleto_tipo		varchar(1),
@boleto_serie		varchar(3),
@boleto_num			varchar(8),
@emision_fecha		smalldatetime,
@emision_hora		varchar(10),
@cajero_cod			smallint,
@cajero_nom			varchar(30),
@pas_nombre_com		varchar(200),
@pas_ruc			varchar(11),
@pas_raz_social		varchar(100),
@pas_direccion		varchar(200),
@nom_ori_pas		varchar(60),
@nom_des_pas		varchar(60),
@doc_tipo			tinyint,
@doc_numero			varchar(20),
@precio_can			real,
@precio_des			varchar(200),
@nom_servicio		varchar(50),
@num_asiento		varchar(2),
@fecha_viaje		smalldatetime,
@embarque_dir		varchar(200),
@embarque_hora		varchar(10),
@codigo_x_fe		varchar(100),
@link_pag_fe		varchar(100),
@cod_poliza			varchar(25),
@cod_terminal		varchar(1),
@tip_impresora		tinyint
)
AS

BEGIN

	INSERT INTO tb_impresion (
		id_venta,
		nom_tip_venta,
		emp_ruc,
		emp_raz_social,
		emp_direccion,
		emp_dir_agencia,
		emp_telefono1,
		emp_telefono2,
		cod_documento,
		boleto_tipo,
		boleto_serie,
		boleto_num,
		emision_fecha,
		emision_hora,
		cajero_cod,
		cajero_nom,
		pas_nombre_com,
		pas_ruc,
		pas_raz_social,
		pas_direccion,
		nom_ori_pas,
		nom_des_pas,
		doc_tipo,
		doc_numero,
		precio_can,
		precio_des,
		nom_servicio,
		num_asiento,
		fecha_viaje,
		embarque_dir,
		embarque_hora,
		codigo_x_fe,
		link_pag_fe,
		cod_poliza,
		cod_terminal,
		tip_impresora	
	)
	VALUES (
		@id_venta,	
		@nom_tip_venta,
		@emp_ruc,
		@emp_raz_social,
		@emp_direccion,
		@emp_dir_agencia,
		@emp_telefono1,
		@emp_telefono2,
		@cod_documento,
		@boleto_tipo,
		@boleto_serie,
		@boleto_num,
		@emision_fecha,
		@emision_hora,
		@cajero_cod,
		@cajero_nom,
		@pas_nombre_com,
		@pas_ruc,
		@pas_raz_social,
		@pas_direccion,
		@nom_ori_pas,
		@nom_des_pas,
		@doc_tipo,
		@doc_numero,
		@precio_can,
		@precio_des,
		@nom_servicio,
		@num_asiento,
		@fecha_viaje,
		@embarque_dir,
		@embarque_hora,
		@codigo_x_fe,
		@link_pag_fe,
		@cod_poliza,
		@cod_terminal,
		@tip_impresora		
	);

END;
